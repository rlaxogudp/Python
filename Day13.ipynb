    "# flask server 구동 코드\n",
    "from flask import Flask\n",
    "from flask import render_template #html 문서를 load\n",
    "# templates 폴더에 있는 html만 바라볼 수 있다\n",
    "app = Flask(__name__)\n",
    "\n",
    "@app.route(\"/exam04\")\n",
    "def exam04():\n",
    "    return render_template(\"exam04.html\")\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    app.run()"
   ]
